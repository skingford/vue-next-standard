{
  "name": "vue3-admin-template",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --mode development",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview --port 8080",
    "svgo": "svgo -f src/assets/icons/svg --config=src/icons/svgo.yml"
  },
  "dependencies": {
    "@vueuse/core": "^5.2.0",
    "axios": "^0.21.1",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.10.6",
    "echarts": "^5.1.2",
    "element-plus": "^1.0.2-beta.65",
    "lodash-es": "^4.17.21",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "pinia": "2.0.0-beta.5",
    "qs": "^6.10.1",
    "vue": "^3.1.5",
    "vue-router": "^4.0.10",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/node": "^16.4.10",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "@vitejs/plugin-legacy": "^1.5.1",
    "@vitejs/plugin-vue": "^1.3.0",
    "@vitejs/plugin-vue-jsx": "^1.1.7",
    "@vue/compiler-sfc": "^3.1.5",
    "@vue/eslint-config-prettier": "^6.0.0",
    "autoprefixer": "^10.3.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-define-config": "^1.0.9",
    "eslint-plugin-vue": "^7.15.1",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "sass": "^1.37.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-webpack-plugin": "^3.0.1",
    "svg-sprite-loader": "^6.0.9",
    "typescript": "^4.3.5",
    "vite": "^2.4.4",
    "vite-plugin-compression": "^0.3.3",
    "vite-plugin-html": "^2.0.7",
    "vite-plugin-imagemin": "^0.4.3",
    "vite-plugin-pwa": "^0.9.3",
    "vite-plugin-style-import": "^1.1.0",
    "vite-plugin-svg-icons": "^1.0.4",
    "vite-plugin-windicss": "^1.2.7",
    "vue-eslint-parser": "^7.10.0",
    "vue-tsc": "^0.2.2",
    "windicss": "^3.1.6"
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,vue}": [
      "prettier --tab-width 2 --write",
      "vite lint --fix",
      "git add"
    ]
  }
}
